[config]

server_name=mgi-testweb3
server_arch=linux

collection_types=load,memory,disk,network

load_types=load,uptime,users

load_freq=60
uptime_freq=60
users_freq=60

memory_types=mem,swap

mem_freq=60
swap_freq=60

disk_types=speed,size
disk_volumes=/vol/stuff,/vol/stuff2,/vol/stuff3
speed_freq=60
size_freq=60

network_types=errors,bandwidth
network_interfaces=eth0,eth1,eth2

errors_freq=60
bandwidth_freq=60



